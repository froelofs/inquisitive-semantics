@article{Clifton:10,
	abstract = {Conventionalist theories of scalar implicature differ from other accounts in that they predict strengthening of embedded scalar terms. Geurts and Pouscoulous (2009) argue that experimental support for this prediction is largely based on sentence comprehension tasks that inflate the frequency with which terms like 'some' are strengthened. Using a picture verification task, they observed no strengthening of embedded scalars. We present data from a multiple-choice picture verification task that is more sensitive to interpretation preferences, and find that readers do show a preference for strengthened interpretations even in embedded phrases. These data cast doubt on Geurts and Pouscoulous's empirical arguments against the existence of embedded implicatures. doi:10.3765/sp.3.7 BibTeX info},
	author = {Charles Clifton and Chad Dube},
	date-added = {2018-07-02 20:54:07 +0000},
	date-modified = {2018-07-02 20:54:07 +0000},
	doi = {10.3765/sp.3.7},
	journal = {Semantics and Pragmatics},
	keywords = {implicature; scalar terms; experimental evidence},
	number = {7},
	pages = {1--13},
	publisher = {Semantics and Pragmatics},
	title = {Embedded implicatures observed: a comment on {G}eurts and {P}ouscoulous (2009)},
	volume = {3},
	year = {2010},
	Bdsk-Url-1 = {http://semprag.org/article/view/sp.3.7}}

